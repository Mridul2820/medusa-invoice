{
  "name": "medusa-invoice",
  "version": "0.0.65",
  "description": "Generate invoice from Medusa",
  "main": "dist/index.js",
  "author": "Mridul",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mridul2820/medusa-invoice"
  },
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "copy-assets": "cp -rv src/fonts dist/fonts",
    "dev": "cross-env npm run build && medusa develop",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.server.json",
    "build:admin": "cross-env medusa-admin build",
    "prepare": "cross-env NODE_ENV=production npm run build:server && medusa-admin bundle && npm run copy-assets"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "11.13.0",
    "@medusajs/admin": "7.1.17",
    "@medusajs/icons": "1.2.2",
    "@medusajs/ui": "3.0.1",
    "@medusajs/utils": "1.11.11",
    "@mui/material": "^5.15.3",
    "@tanstack/react-query": "4.22",
    "ajv": "^8.17.1",
    "dayjs": "^1.11.13",
    "lodash": "4.17.21",
    "medusa-interfaces": "^1.3.7",
    "medusa-react": "9.0.18",
    "moment": "^2.30.1",
    "pdfkit": "0.15.0",
    "react-hook-form": "^7.49.2",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@medusajs/medusa": "1.20.10",
    "@types/express": "^4.17.13",
    "@types/react-router-dom": "^5.3.3",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": "1.20.10",
    "react": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "resolutions": {
    "@types/mime": "3.0.4",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
